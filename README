NAME
    Bundle::SYP - SYP's cozy environment

VERSION
    version 0.5

SYNOPSIS
     # Ubuntu Server 10.04.2 LTS
     apt-get install curl gcc git-core libcurl4-gnutls-dev libexpat-dev libncurses-dev libreadline-dev

     perl -MCPAN -e 'install Bundle::SYP'

